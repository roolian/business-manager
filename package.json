    "@types/react-dom": "^19.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "hot-hook": "^0.3.1",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.16",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.6.3",
    "vite": "^7.0.4"
