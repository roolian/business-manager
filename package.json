  "overrides": {
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "strtok3": "8.0.1"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "better-sqlite3",
      "esbuild"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
