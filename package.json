    "@adonisjs/session": "^7.5.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^4.0.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@inertiajs/react": "^1.2.0",
    "@types/lodash": "^4.17.20",
    "@vinejs/vine": "^2.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "better-sqlite3": "^11.7.0",
    "clsx": "^2.1.1",
