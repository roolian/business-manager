{
  "name": "adonis",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
